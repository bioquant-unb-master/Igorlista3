#módulo 3
install.packages("MASS")
library("MASS")
str(crabs)
View(crabs)
data.frame(crabs)
data(crabs)
model <- lm(FL ~ sp + sex, data = crabs)
anova(model)
summary(model)
plot(FL ~ sex+sp, data = crabs)
boxplot(FL ~ sex + sp, data = crabs, xlab = "Sex + Sp",ylab = "Lóbulo frontal", main = "análise de variancia")
model2 <- lm(FL ~ sp * sex, data = crabs)
anova(model2)
summary(model2)
install.packages("lawstat")
library(lawstat)
shapiro.test(model$residuals)
shapiro.test(model2$residuals)
levene.test(crabs$FL, group = crabs$sex)
levene.test(crabs$FL, group = crabs$sp)
